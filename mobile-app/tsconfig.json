{
  "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "strict": true,
      "jsx": "react",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "paths": {
        "@/*": [
          "./*"
        ]
      }
    },
    "include": ["**/*.ts", "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"],
    "exclude": ["node_modules"]
  }

